{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run DemoApplication-HTML5Module",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/demoapplication-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsHTML5Module/index.html",
        "--ui5",
        "{\"version\":\"1.78.17\"}"
      ],
      "cwd": "${workspaceFolder}/HTML5Module",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/DemoApplication/HTML5Module\"}"
      },
      "envFile": "${workspaceFolder}/HTML5Module/.env2",
      "preLaunchTask": "Build DemoApplication"
    }
  ]
}